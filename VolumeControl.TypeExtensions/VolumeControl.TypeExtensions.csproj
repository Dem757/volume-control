<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <UseWindowsForms>True</UseWindowsForms>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>VolumeControl.SDK.TypeExtensions</PackageId>
    <Copyright>GNU General Public License v3 </Copyright>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageTags>volume-control;VolumeControl;volume-control-sdk</PackageTags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/radj307/volume-control</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>iconSilvered.png</PackageIcon>
    <PackageProjectUrl>https://radj307.github.io/volume-control/html/index.html</PackageProjectUrl>
    <Authors>radj307</Authors>
    <NeutralLanguage>en-CA</NeutralLanguage>
    <PackageReleaseNotes>
			Full Changelog:
			https://github.com/radj307/volume-control/commits/main
		</PackageReleaseNotes>
    <Version>5.2.0</Version>
    <ExtendedVersion>5.2.0</ExtendedVersion>
    <ReleaseType>NORMAL</ReleaseType>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\VolumeControl\Resources\images\iconSilvered.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Semver" Version="2.2.0" />
    <PackageReference Include="System.Management" Version="6.0.0" />
  </ItemGroup>
</Project>